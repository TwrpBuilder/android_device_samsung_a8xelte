/boot		emmc		/dev/block/sda5
/cache		ext4		/dev/block/sda15	
/data		ext4		/dev/block/sd17 	length=-16384
/recovery	emmc		/dev/block/sda6	
/system		ext4		/dev/block/sda14	
/efs		emmc		/dev/block/sda3                                flags=display="EFS";backup=1	
/radio		emmc		/dev/block/sda8                                flags=display="RADIO";backup=1	
/external_sdcard vfat           /dev/block/mmcblk1p1      /dev/block/mmcblk1   flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg         vfat           /dev/block/sdd1	          /dev/block/sda       flags=display="USB-OTG";storage;wipeingui;removable
